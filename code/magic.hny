const N = 4

sq = [ permute({ 1 .. N }) for _ in { 1 .. N } ]

def is_a_solution() returns good:
    good = True
    for i in { 0 .. N-1 }:
        if { sq[i][j] for j in { 0 .. N-1 } } != { 1 .. N }:
            good = False
    for i in { 0 .. N-1 }:
        if { sq[j][i] for j in { 0 .. N-1 } } != { 1 .. N }:
            good = False

assert not is_a_solution()
